{
  "name": "Basic 2F toilet checker",
  "version": "1.0",
  "description": "Check status of Basic 2F toilet",
  "permissions": ["activeTab","declarativeContent", "storage", "alarms"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/toilet_disable.png"
    },
  "manifest_version": 2
}
